{
    "name": "scaleo",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "ng": "nx",
        "nx:graph": "nx dep-graph",
        "nx:graph-watch": "nx dep-graph --watch",
        "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
        "start": "nx serve",
        "build": "nx build",
        "build:prod": "nx build --prod",
        "lint": "nx lint",
        "test": "nx test",
        "affected:apps": "nx affected:apps",
        "affected:libs": "nx affected:libs",
        "affected:build": "nx affected:build",
        "affected:build-all": "nx affected:build --all",
        "affected:lint": "nx affected:lint --fix --parallel --uncommitted",
        "open:local-build": "http-server dist/apps/scaleo",
        "open:build-stats": "ng build --stats-json",
        "build:analyzer": "webpack-bundle-analyzer dist/stats.json",
        "prettier": "prettier --write {apps,libs}/**/*.{js,ts}",
        "stylelint": "stylelint --fix {apps,libs}/**/*.{ts,css,scss}",
        "format:fix": "pretty-quick --staged",
        "precommit": "run-s format:fix lint-staged"
    },
    "lint-staged": {
        "*.{ts}, *.{html}": [
            "prettier --write",
            "eslint --fix"
        ],
        "*.{css,scss}": "stylelint --fix"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "private": true,
